{
  "name": "react-semantic-breadcrumbs",
  "version": "1.0.0",
  "repository": "BradEstey/react-semantic-breadcrumbs",
  "author": "Brad Estey",
  "license": "MIT",
  "main": "lib",
  "files": [
    "*.js",
    "*.md",
    "umd"
  ],
  "scripts": {
    "build": "node ./scripts/build.js",
    "build-lib": "rimraf lib && babel ./modules -d . --ignore '__tests__'",
    "build-umd": "webpack modules/index.js umd/react-semantic-breadcrumbs.js",
    "build-min": "webpack -p modules/index.js umd/react-semantic-breadcrumbs.min.js",
    "prepublish": "node ./scripts/build.js",
    "clean": "rm $(ls modules | grep -v __)",
    "lint": "eslint modules",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-react": "^6.0.0",
    "gzip-size": "^3.0.0",
    "in-publish": "^2.0.0",
    "jest-cli": "^15.1.1",
    "pretty-bytes": "^3.0.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.0",
    "react-test-renderer": "^15.3.1",
    "readline-sync": "^1.4.4",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "description": "A React component for rendering semantic breadcrumb lists.",
  "keywords": [
    "react",
    "breadcrumbs",
    "semantic",
    "rdfa",
    "json-ld",
    "schema.org",
    "microdata",
    "microformats"
  ]
}
